{#
	{% render '@partial-favicons' %}
#}
<!-- favicon -->
<link rel="apple-touch-icon" sizes="180x180" href="{{ '/favicons/apple-touch-icon.png'|path }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ '/favicons/favicon-32x32.png'|path }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ '/favicons/favicon-16x16.png'|path }}">
<link rel="manifest" href="{{ '/favicons/site.webmanifest'|path }}">
<link rel="mask-icon" href="{{ '/favicons/safari-pinned-tab.svg'|path }}" color="{{ config.color }}">
<link rel="shortcut icon" href="{{ '/favicons/favicon.ico'|path }}">
<meta name="msapplication-TileColor" content="{{ config.color }}">
<meta name="msapplication-TileImage" content="{{ '/favicons/mstile-144x144.png'|path }}">
<meta name="msapplication-config" content="{{ '/favicons/browserconfig.xml'|path }}">
<meta name="theme-color" content="{{ config.color }}">